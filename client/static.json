{
  "root": "build/",
  "clean_urls": false,
  "routes": {
    "/reset-password/*": "index.html",
    "/**": "index.html"
  },
  "headers": {
    "/**": {
      "Cache-Control": "no-store, no-cache",
      "Strict-Transport-Security": "max-age=31536000; includeSubDomains",
      "X-Frame-Options": "DENY"
    }
  }
} 